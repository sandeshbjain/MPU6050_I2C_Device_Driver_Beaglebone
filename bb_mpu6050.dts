/dts-v1/;
/plugin/;

/	{
		compatible = "ti,beaglebone-black";
		
		/* Adding i2c configuration*/
		fragment@0{
			target = <&i2c2>;
		
			__overlay__ {
				#address-cells=<1>;
				#size-cells=<0>;
				status="okay";
				clock-frequency = <400000>;
			
				mpu6050: mpu6050@68{
					compatible = "invesense,mpu6050";
					reg= <0x68>;
				};
			};
		};
	};	
